        <!-- ARTICLE: THE CAREHUB APPROACH                -->
        <!-- ============================================ -->
        <section class="py-16 lg:py-20 bg-white" aria-label="From silos to insight: the CareHub approach">
            <div class="max-w-3xl mx-auto px-4 sm:px-6 lg:px-8">
